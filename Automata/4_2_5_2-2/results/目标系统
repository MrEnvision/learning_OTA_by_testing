digraph {
	1 [label=1]
	2 [label=2 shape=doublecircle]
	3 [label=3 shape=doublecircle]
	4 [label=4 shape=doublecircle]
	1 -> 3 [label=" b [0,3] True"]
	1 -> 4 [label=" a [1,+) True"]
	4 -> 1 [label=" b [1,+) False"]
	4 -> 3 [label=" a [2,3) True"]
	3 -> 2 [label=" b [0,1] True"]
	3 -> 4 [label=" a [0,4] True"]
	2 -> 1 [label=" a (1,+) True"]
}
